    <queryString>
        <![CDATA[-- ------------------------------------------------------------------------------------------------
SELECT
-- Добавляем значение для группировки
bcType.value AS BC_TYPE,

-- Остаток на начало периода
TRUNCATE(IFNULL((BALANCE_ON_START.value/1000), 0),3) AS BALANCE_ON_START,
-- Приход закупленных КК
TRUNCATE(IFNULL((INCOME.self/1000), 0),3) AS INCOME_SELF,
-- Приход закупленных КК
TRUNCATE(IFNULL((INCOME.other/1000), 0),3) AS INCOME_OTHER,
-- Расход собственных КК
TRUNCATE(IFNULL((OUTCOME.self/1000), 0),3) AS OUTCOME_SELF,
-- Расход купленных КК
TRUNCATE(IFNULL((OUTCOME.other/1000), 0),3) AS OUTCOME_OTHER,
-- Брак
TRUNCATE(IFNULL((DEFECT.value/1000), 0),3) AS DEFECT,
-- Добавочный объем от вирусинактиваций в экспедиции
TRUNCATE(IFNULL((VOLUME_ADDED.self/1000), 0),3) AS VOLUME_ADDED_SELF,
TRUNCATE(IFNULL((VOLUME_ADDED.other/1000), 0),3) AS VOLUME_ADDED_OTHER,
-- Подсчеты
TRUNCATE((IFNULL((BALANCE_ON_START.value), 0) +
IFNULL((VOLUME_ADDED.self), 0) +
IFNULL((VOLUME_ADDED.other), 0) +
IFNULL((INCOME.self), 0) +
IFNULL((INCOME.other), 0) -
IFNULL((OUTCOME.self), 0) -
IFNULL((OUTCOME.other), 0) -
IFNULL((DEFECT.value), 0))/1000,3)
AS BALANCE_ON_END_COUNTED

-- ------------------------------------------------------------------------------------------------
-- Выбираем из списка типов КК
FROM trfu_blood_component_types bcType
-- Остаток на начало периода ----------------------------------------------------------------------
LEFT JOIN
(
  SELECT
	   sum(vol) AS value, BC_TYPE_ID

  FROM

(SELECT
  comp.id AS iid, comp.volume AS vol, comp.componentType_id AS BC_TYPE_ID
  FROM
    trfu_blood_components comp
    INNER JOIN trfu_blood_component_history hist ON comp.id = hist.component_id
    INNER JOIN wf_history wf ON hist.history_entry_id = wf.id
  WHERE
    wf.to_status_id = 3
    AND wf.startDate < ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND)
    AND wf.startDate = (
      SELECT max(wf_in.startDate)
      FROM
        trfu_blood_component_history hist_in
        INNER JOIN wf_history wf_in ON hist_in.history_entry_id = wf_in.id
      WHERE
        (wf_in.startDate) < ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND)
        AND hist_in.component_id = comp.id
      )
  	AND (comp.status_id <> 100 OR (comp.status_id = 100 and comp.splitDate >=ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND)))
and comp.deleted=0
	) AS table11

LEFT JOIN

(SELECT comp.id AS iid
FROM trfu_blood_components comp
	INNER JOIN trfu_blood_component_history hist ON comp.id = hist.component_id
	INNER JOIN wf_history wf ON hist.history_entry_id = wf.id
WHERE
  wf.from_status_id=3
  AND comp.status_id=wf.to_status_id
  AND comp.status_id IN (-2,10)
  AND wf.startDate=(
      SELECT max(wf_in.startDate)
      FROM
        trfu_blood_component_history hist_in
        INNER JOIN wf_history wf_in ON hist_in.history_entry_id = wf_in.id
      WHERE
        wf_in.startDate <= ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND)
        AND hist_in.component_id = comp.id)
  AND comp.deleted = 0
) AS table21

ON table21.iid=table11.iid
WHERE table21.iid IS null
GROUP BY BC_TYPE_ID
) AS BALANCE_ON_START ON BALANCE_ON_START.BC_TYPE_ID = bcType.id
-- Приход КК --------------------------------------------------------------------------
LEFT JOIN
(
	SELECT
  	SUM(IF(comp.maker_id = 11, comp.volume, 0)) AS self,
  	SUM(IF(comp.maker_id <> 11, comp.volume, 0)) AS other,
    comp.componentType_id AS BC_TYPE_ID
	from trfu_blood_components comp
  	inner join trfu_blood_component_history hist on comp.id = hist.component_id
  	inner join wf_history wf on hist.history_entry_id = wf.id
where wf.to_status_id = 3
	and wf.startDate between ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND) and ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
	and comp.deleted = 0
	and comp.split = false
	and 1 = (select
				count(sub_comp.id)
			from trfu_blood_components sub_comp
				inner join trfu_blood_component_history sub_hist on sub_comp.id = sub_hist.component_id
				inner join wf_history sub_wf on sub_hist.history_entry_id = sub_wf.id
			where sub_wf.to_status_id = 3
               	and sub_wf.startDate <= wf.startDate
				and sub_comp.id = comp.id)
		GROUP BY comp.componentType_id
) AS INCOME ON INCOME.BC_TYPE_ID = bcType.id

-- Расход -----------------------------------------------------------------------------------------
LEFT JOIN
(
	SELECT
  	SUM(IF(comp.maker_id = 11, comp.volume, 0)) AS self,
  	SUM(IF(comp.maker_id <> 11, comp.volume, 0)) AS other,
  	comp.componentType_id AS BC_TYPE_ID
  from trfu_blood_components comp
  	inner join trfu_blood_component_history hist on comp.id = hist.component_id
  	inner join wf_history wf on hist.history_entry_id = wf.id
  where wf.to_status_id = 10
  	and wf.startDate between ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND) and  ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
  	and comp.deleted = 0
  	and comp.id not in (
      select
  			sub_comp.id
  		from trfu_blood_components sub_comp
  			inner join trfu_blood_component_history sub_hist on sub_comp.id = sub_hist.component_id
  			inner join wf_history sub_wf on sub_hist.history_entry_id = sub_wf.id
  		where sub_wf.to_status_id <> 10
  			and sub_wf.startDate between wf.startDate and  ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
  			and sub_comp.id = comp.id)
  GROUP BY comp.componentType_id
) AS OUTCOME ON OUTCOME.BC_TYPE_ID = bcType.id
-- Брак -------------------------------------------------------------------------------------------
LEFT JOIN
(
	SELECT
    SUM(comp.volume) AS value,
	  comp.componentType_id AS BC_TYPE_ID
	FROM trfu_blood_components comp
	  INNER JOIN trfu_blood_component_history hist ON comp.id = hist.component_id
	  INNER JOIN wf_history wf ON hist.history_entry_id = wf.id
	WHERE
  	comp.deleted=0
  	AND DATE(wf.startDate) >= ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND)
  	AND DATE(wf.startDate) <= ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
  	AND wf.to_status_id = -2
  	AND comp.status_id = -2
  GROUP BY comp.componentType_id
) AS DEFECT ON DEFECT.BC_TYPE_ID = bcType.id
-- Добавочный объем от вирусинактиваций в экспедиции ----------------------------------------------------
LEFT JOIN
(
	SELECT
  	SUM(IF(tbc.maker_id = 11, tbc.volume-tbc.preInactivatedVolume, 0)) AS self,
  	SUM(IF(tbc.maker_id <> 11, tbc.volume-tbc.preInactivatedVolume, 0)) AS other,
	  tbc.componentType_id AS BC_TYPE_ID
	FROM
    trfu_blood_components tbc
    INNER JOIN trfu_blood_component_history tbch ON tbc.id = tbch.component_id
    INNER JOIN wf_history wh ON tbch.history_entry_id = wh.id

  WHERE
    wh.from_status_id=wh.to_status_id
    AND wh.to_status_id=3
    AND wh.action_id=14
    AND wh.startDate BETWEEN ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND) and ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
    AND tbc.deleted=0 
    AND tbc.virusInactivationDate  BETWEEN ADDDATE(DATE($P{StartDate}), INTERVAL '00:00:00' HOUR_SECOND) and ADDDATE(DATE($P{EndDate}), INTERVAL '23:59:59' HOUR_SECOND)
    AND tbc.split = TRUE
  GROUP BY tbc.componentType_id
) AS VOLUME_ADDED ON VOLUME_ADDED.BC_TYPE_ID = bcType.id

-- ------------------------------------------------------------------------------------------------

WHERE

(
	BALANCE_ON_START.value <> 0
	OR
	INCOME.self <> 0
	OR
	INCOME.other <> 0
	OR
	OUTCOME.self <> 0
	OR
	OUTCOME.other <> 0
	OR
	DEFECT.value <> 0
  OR
  VOLUME_ADDED.self <> 0
  OR
  VOLUME_ADDED.other <> 0
)

 GROUP BY BC_TYPE]]>
    </queryString>
    <field name="BC_TYPE" class="java.lang.String"/>
    <field name="BALANCE_ON_START" class="java.math.BigDecimal"/>
    <field name="INCOME_SELF" class="java.math.BigDecimal"/>
    <field name="INCOME_OTHER" class="java.math.BigDecimal"/>
    <field name="OUTCOME_SELF" class="java.math.BigDecimal"/>
    <field name="OUTCOME_OTHER" class="java.math.BigDecimal"/>
    <field name="DEFECT" class="java.math.BigDecimal"/>
    <field name="VOLUME_ADDED_SELF" class="java.math.BigDecimal"/>
    <field name="VOLUME_ADDED_OTHER" class="java.math.BigDecimal"/>
    <field name="BALANCE_ON_END_COUNTED" class="java.math.BigDecimal"/>
    <variable name="SELF_INCOME" class="java.math.BigDecimal">
        <variableExpression><![CDATA[$F{INCOME_SELF}.add($F{VOLUME_ADDED_SELF})]]></variableExpression>
    </variable>
    <variable name="OTHER_INCOME" class="java.math.BigDecimal">
        <variableExpression><![CDATA[$F{INCOME_OTHER}.add($F{VOLUME_ADDED_OTHER})]]></variableExpression>
    </variable>
    <variable name="TOTAL_INCOME" class="java.math.BigDecimal">
        <variableExpression><![CDATA[$V{SELF_INCOME}.add($V{OTHER_INCOME})]]></variableExpression>
    </variable>
    <variable name="GRAND_TOTAL_INCOME" class="java.math.BigDecimal">
        <variableExpression><![CDATA[$V{TOTAL_INCOME}.add($F{BALANCE_ON_START})]]></variableExpression>
    </variable>
    <variable name="TOTAL_OUTCOME" class="java.math.BigDecimal">
        <variableExpression><![CDATA[$F{OUTCOME_SELF}.add($F{OUTCOME_OTHER})]]></variableExpression>
    </variable>
