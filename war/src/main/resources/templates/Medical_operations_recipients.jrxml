	<queryString>
		<![CDATA[SELECT
     count(trfu_medical_operations.`id`) AS medical_operations_count,
     trfu_classifiers.`value` AS trfu_classifiers_value,
     trfu_medical_operations.`recipient` AS trfu_medical_operations_recipient,
     trfu_medical_donors.`firstName` as firstName,
     trfu_medical_donors.`lastName` as lastName,
     trfu_medical_donors.`middleName` as middleName,
     trfu_medical_operations.`donor_id` AS trfu_medical_operations_donor_id
FROM
     `trfu_classifiers` trfu_classifiers INNER JOIN `trfu_medical_operations` trfu_medical_operations ON trfu_classifiers.`id` = trfu_medical_operations.`operationForm_id`
inner join `trfu_medical_donors` trfu_medical_donors on trfu_medical_operations.`donor_id` = trfu_medical_donors.`id`
WHERE
     DATE(trfu_medical_operations.`created`) >= DATE($P{StartDate})
 AND DATE(trfu_medical_operations.`created`) <= DATE($P{EndDate})
 AND trfu_medical_operations.`deleted` = 0
GROUP BY
 trfu_medical_operations.`recipient`,     trfu_medical_donors.`id`,trfu_medical_operations.`operationForm_id`
order by trfu_medical_operations.`donor_id`]]>
	</queryString>
