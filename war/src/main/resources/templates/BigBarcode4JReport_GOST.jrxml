	<subDataset name="DonationAnalisys" uuid="632737b0-b2eb-4eaf-87be-a95d7adc2022">
		<parameter name="docId" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
concat_ws(': ',anal_appoint.value,tests.value) AS donationTests
FROM
trfu_blood_components comp INNER JOIN trfu_blood_donation_requests req ON comp.donationId = req.id
INNER JOIN trfu_external_appointment_tests appoint_test ON req.appointment_id=appoint_test.appointment_id
INNER JOIN trfu_tests tests ON tests.id=appoint_test.test_id
INNER JOIN trfu_analysis_types anal_appoint ON (anal_appoint.id=tests.type_id AND anal_appoint.category="Донация")
WHERE comp.id=$P{docId} AND anal_appoint.id IN ('18','19','20','21','22')
AND comp.deleted = "0"
AND req.deleted = "0"

UNION

SELECT
concat_ws(': ',anal_req.value,req_tests.value) AS donationTests
FROM
trfu_blood_components comp INNER JOIN trfu_blood_donation_requests req ON comp.donationId = req.id
INNER JOIN trfu_blood_donation_requests_trfu_tests req_test ON req_test.trfu_blood_donation_requests_id=req.id
INNER JOIN trfu_tests req_tests ON req_test.tests_id=req_tests.id
INNER JOIN trfu_analysis_types anal_req ON (anal_req.id=req_tests.type_id AND anal_req.category="Донация")
WHERE comp.id=$P{docId} AND anal_req.id IN ('18','19','20','21','22')
AND comp.deleted = "0"
AND req.deleted = "0"

UNION
-- Для закупленных
SELECT
concat_ws(': ',anal_req.value,req_tests.value) AS donationTests
FROM
trfu_blood_components comp 
INNER JOIN trfu_blood_component_tests comp_test ON comp_test.component_id = comp.id
INNER JOIN trfu_tests req_tests ON comp_test.test_id = req_tests.id
INNER JOIN trfu_analysis_types anal_req ON (anal_req.id=req_tests.type_id AND anal_req.category="Донация")
WHERE comp.id=$P{docId} AND anal_req.id IN ('18','19','20','21','22')
AND comp.deleted = "0"
AND comp.purchased]]>
		</queryString>
		<field name="donationTests" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DonationPhenotypes" uuid="95547d5b-d47f-4873-83f1-7113e60f54a8">
		<parameter name="docId" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
				GROUP_CONCAT(CONCAT(IF(anal_req.value = 'K', '   K', anal_req.value),req_tests.value) SEPARATOR '') as donationPhenotypes
				FROM
				trfu_blood_components comp 
				-- Для не закупленных
				LEFT join trfu_blood_donation_requests req on (NOT comp.purchased) AND comp.donationId = req.id
				LEFT join trfu_blood_donation_request_tests_immuno req_test on req_test.trfu_blood_donation_requests_id=req.id
				-- для закупленных
				LEFT JOIN trfu_blood_component_tests bct ON comp.purchased AND comp.id = bct.component_id
				
				LEFT join trfu_tests req_tests on req_tests.id = IF(comp.purchased, bct.test_id, req_test.testsImmuno_id)
				LEFT join trfu_analysis_types anal_req on anal_req.id=req_tests.type_id
				where comp.id=$P{docId} and anal_req.category="Иммуносерология"
				ORDER BY anal_req.id]]>
		</queryString>
		<field name="donationPhenotypes" class="java.lang.String"/>
	</subDataset>
	<parameter name="docId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
    		<![CDATA[SELECT
    	comp.parentNumber AS 'donationNumber',
    	org.shortName AS 'makerInfo',
    	LPAD(org.stateCode, 2, '0') AS 'pp',
    	LPAD(org.organizationCode, 2, '0') AS 'oo',
    	DATE_FORMAT(COALESCE(comp.donationDate, comp.created), "%y") AS 'rr',
    	comp.number AS 'componentNumber',
    	rhesusFactor.value AS 'rhesusFactor',
    	bloodGroup.value AS 'bloodGroupValue',
    	comp.expirationDate AS 'expirationDate',
    	comp.donationDate AS 'donationDate',
    	comp.productionDate AS 'productionDate',
    	anticoagulant.value AS 'anticoagulantType',
    	comp.anticoagulantVolume AS 'anticoagulantVolume',
    	comp.lowerStorageTemperature AS 'storageTemperatureFrom',
    	comp.higherStorageTemperature AS 'storageTemperatureTo',
    	componentType.value AS 'componentType',
    	factDonationResult.dose AS 'donationDose',
    	factDonationResult.donationType_id AS 'donationType',
    	comp.volume AS 'componentVolume',
    	comp.status_id AS 'componentStatus',
    	CONCAT_WS(' ', resuspend.value, comp.preservativeVolume, 'мл')  AS 'resuspendInfo',
    	COALESCE(comp.donorCode, donor.number) AS 'donorCode',
    	comp.irradiated AS 'irradiated',
    	comp.leukoFiltered AS 'leukoFiltered',
    	comp.inactivated AS 'inactivated',
    	comp.quarantineFinishDate AS 'quarantineFinishDate'
    FROM
    trfu_blood_components comp
    LEFT JOIN crm_contragents org ON comp.maker_id = org.id
    LEFT JOIN trfu_classifiers rhesusFactor ON comp.rhesusFactor_id = rhesusFactor.id
    LEFT JOIN trfu_blood_groups bloodGroup ON bloodGroup.id = comp.bloodGroup_id
    LEFT JOIN trfu_anticoagulants anticoagulant ON anticoagulant.id = comp.anticoagulant_id
    INNER JOIN trfu_blood_component_types componentType ON comp.componentType_id = componentType.id
    -- Для НЕ закупленных
    LEFT JOIN trfu_blood_donation_requests donation ON donation.id = comp.donationId
    LEFT JOIN trfu_donors donor ON donor.id = donation.donor_id
    LEFT JOIN trfu_blood_donation_request_fact_entries factDonationEntries ON  (NOT comp.purchased) AND comp.donationId = factDonationEntries.donation_id
    -- Для закупленных
    LEFT JOIN trfu_blood_component_entry purchasedDonationEntries ON comp.purchased AND comp.id = purchasedDonationEntries.component_id
    -- параметры кроводачи для обоих видов (закупленных и не закупленных)
    LEFT JOIN trfu_blood_donation_entries factDonationResult ON factDonationResult.id = IF(comp.purchased, purchasedDonationEntries.entry_id, factDonationEntries.entry_id)
    -- ресуспендирующий раствор
    LEFT JOIN trfu_classifiers resuspend ON comp.preservative_id = resuspend.id
    WHERE comp.id = $P{docId}]]>
    	</queryString>
